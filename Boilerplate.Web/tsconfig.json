{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./wwwroot/dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "types": [
      "webpack-env"
    ],
    "paths": {
      "@utils/*": ["./App/utils/*"]
    }
  },
  "include": [
    "./App/**/*"
  ]
}